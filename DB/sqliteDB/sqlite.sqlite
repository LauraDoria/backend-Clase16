const {options} = require('../options/sqliteDB.js')
const knex = require('knex')(options)

knex.schema.createTable('websocketChatAppHistory', table => {
    table.increments('id')
    table.string('user')
    table.string('message')
    table.string('date')
}).then(
    () => console.log('New table created')
).catch(
    (error) => console.error(error)
).finally(() => {
    knex.destroy()
})